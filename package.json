{
  "name": "umi-simple-web-template",
  "version": "1.2.0",
  "private": true,
  "repository": "https://github.com/arvinxx/umi-simple-web-template.git",
  "scripts": {
    "build": "max build",
    "build:analyze": "ANALYZE=1 max build",
    "dev": "max dev",
    "lint": "max lint",
    "prepare": "husky install",
    "release": "semantic-release",
    "start": "vercel dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest -u"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "antd": "^5",
    "antd-style": "^3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@emotion/jest": "^11",
    "@testing-library/react": "^13",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@umijs/max": "^4",
    "@vitest/coverage-c8": "latest",
    "commitlint": "^17",
    "commitlint-config-gitmoji": "^2",
    "cross-env": "^7",
    "eslint": "^8",
    "husky": "^8",
    "jsdom": "^21",
    "lint-staged": "^13",
    "prettier": "^2",
    "semantic-release": "^20",
    "semantic-release-config-gitmoji": "^1",
    "typescript": "^5",
    "vercel": "^28",
    "vitest": "latest"
  }
}
